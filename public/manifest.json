{
  "manifest_version": 3,
  "name": "IntelliSwipe Advisor",
  "version": "0.1.0",
  "description": "Your AI-powered dating assistant for Tinder.",
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "https://tinder.com/*"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://tinder.com/*"],
      "js": ["assets/content.js"]
    }
  ]
}